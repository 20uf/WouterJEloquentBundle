{
    "name": "wouterj/eloquent-bundle",
    "description": "Implementing the Eloquent ORM into Symfony2",
    "authors": [
        {
            "name": "Wouter J",
            "email": "wouter@wouterj.nl"
        }
    ],
    "license": "MIT",

    "require": {
        "illuminate/database": "~5.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "0.*",
        "symfony/symfony": "~2.3",
        "phpspec/prophecy": "1.1.*",
        "illuminate/cache": "~5.0",
        "illuminate/console": "5.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,

    "autoload": {
        "psr-4": { "WouterJ\\EloquentBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
